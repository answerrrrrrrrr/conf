# Safe rm (http://blog.csdn.net/wklken/article/details/6898590)
mkdir -p ~/.trash
alias rm=trash
alias lstrash='ls ~/.trash'
trash() {
    mv $@ ~/.trash/
}
untrash() {
    mv -i ~/.trash/$@ ./
}
cleartrash() {
    read -p "Clear trash?[y/N]" confirm
    [ $confirm == 'y'] || [ $confirm == 'Y' ] && /usr/bin/rm -rfv ~/.trash/*
}

